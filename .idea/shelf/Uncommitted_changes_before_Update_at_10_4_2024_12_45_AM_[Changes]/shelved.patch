+# How to Create a JSON file
+import json
+import csv
+
+if __name__ != __main__:
+    file = "01_names.json"
+    newFile = "01_new_json_data.json"
+
+
+    # Pass/Write data to a JSON file
+    def write_to_json(data, filename=newFile):
+        with open(filename, "w") as f:
+            json.dump(data, f, indent=4)
+
+
+    def open_a_json_file:
+        # Open JSON file
+        with open(file, "r") as json_file:
+            data = json.load(json_file)
+            write_json(data)
+
+
+    data = ["Norman", "Lisa"]
+    write_json(data)
+
+    def append_to_json_file
+    # Append new entry to JSON file
+    with open("01_names.json") as json_file:
+        data = json.load(json_file)
+        temp = data["names1"]
+        y = {"firstname": "Frank", "age": 60}
+        temp.append(y)
+
+    write_json(data)
+
+def conv_json_to_csv:
+
+    with open("01_names.json", "r") as f:
+        data = json.load(f)
+        names = data["names1"]
+
+    with open("01_names.csv", "w", newline="") as f:
+        fieldnames = names[0].keys()
+        writer = csv.DictWriter(f, fieldnames=fieldnames)
+        writer.writeheader()
+        for name in names:
+            writer.writerow(name)
