+    'Harry': 88,
+    'Ron': 78,
+    'Hermione': 95,
+    'Draco': 75,
+    'Neville': 60
+}
+
+student_grades = {}
+
+
+def conv_to_grade(name, score_grade):
+    if score_grade >= 91:
+        print(f"{name} Outstanding")
+        student_grades[name] = "Outstanding"
+    elif score_grade >= 81:
+        print(f"{name} Exceeds Expectations")
+        student_grades[name] = "Exceeds Expectations"
+    elif score_grade >= 71:
+        print(f"{name} Acceptable")
+        student_grades[name] = "Acceptable"
+    else:
+        print(f"{name} Fail")
+        student_grades[name] = "Fail"
+
+
+for key in student_scores:
+    conv_to_grade(key, student_scores[key])
+
+print(student_grades) 
